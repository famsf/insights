<div class="footer__cta__wrapper">
  <div class="grid-container grid-container-padded">
    <div class="grid-x grid-margin-x grid-margin-y grid-padding-x grid-padding-y small-up-1 medium-up-2 large-up-3">

      {% set tout = {
          title: footer__cta__left__title,
          blurb: footer__cta__left__description,
          cta__buttom: footer__cta__left__cta
        }
      %}
      {% include "@molecules/footer__cta/footer__cta.html.twig" with tout only %}

      {% set tout = {
          title: footer__cta__middle__title,
          blurb: footer__cta__middle__description,
          cta__buttom: footer__cta__middle__cta
        }
      %}
      {% include "@molecules/footer__cta/footer__cta.html.twig" with tout only %}

      {% set tout = {
          title: footer__cta__right__title,
          blurb: footer__cta__right__description,
          cta__buttom: footer__cta__right__cta,
          classes: 'theme--primary-dark  '
        }
      %}
      {% include "@molecules/footer__cta/footer__cta.html.twig" with tout only %}

    </div>
  </div>
</div>
